#include "iostream"
#include "vector"
using namespace std;

class Matrix
{
public:
    int a, b;
    vector<vector<int>> m;

    Matrix(int a, int b, vector<vector<int>> input) {
        this->a = a;
        this->b = b;
        m.resize(a, vector<int>(b, 0));
        for (int i = 0; i < a; i++) {
            for (int j = 0; j < b; j++) {
                m[i][j] = input[i][j];
            }
        }
    }

    void print()
    {
        for (int i = 0; i < m.size(); i++)
        {
            for (int j = 0; j < m[i].size(); j++)
            {
                cout << m[i][j] << ' ';
            }
            cout << '\n';
        }
    }
    
    Matrix operator+(const Matrix &k)
    {
        Matrix r(a,b,m);
        for (int i = 0; i < a; i++)
        {
            for (int j = 0; j < b; j++)
            {
                r.m[i][j] = this->m[i][j] + k.m[i][j];
            }
        }
        return r;
    };
};

int main()
{
    Matrix mr(2,2,{{1,2},{8,9}});
    mr.print();
    cout<<endl;
    Matrix ma(2,2,{{5,6},{7,3}});
    ma.print();
    cout<<endl;
    Matrix mb= ma+mr;
    mb.print();
    return 0;
   }
